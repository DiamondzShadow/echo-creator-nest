# Supabase Configuration
VITE_SUPABASE_PROJECT_ID="your_project_id"
VITE_SUPABASE_PUBLISHABLE_KEY="your_publishable_key"
VITE_SUPABASE_URL="https://your-project.supabase.co"

# LiveKit Configuration (for instant browser streaming)
VITE_LIVEKIT_URL="wss://your-project.livekit.cloud"

# FVM YouTube Clone Configuration
# Deploy the contract in contracts/YouTube.sol and add the address here
# See FVM_SETUP.md for deployment instructions
VITE_FVM_CONTRACT_ADDRESS="0x..."
# Get your Lighthouse API key from https://files.lighthouse.storage
VITE_LIGHTHOUSE_API_KEY="your_lighthouse_api_key"

# OpenSea MCP Integration
# Get your OpenSea MCP token from https://docs.opensea.io/reference/mcp
VITE_OPENSEA_MCP_TOKEN="your_opensea_mcp_token"
VITE_OPENSEA_API_KEY="your_opensea_api_key"

# Twitch Configuration
VITE_TWITCH_CLIENT_ID="your_twitch_client_id"

# TikTok Configuration
VITE_TIKTOK_CLIENT_KEY="your_tiktok_client_key"

# Edge Function Environment Variables (Set in Supabase Dashboard)
# TIKTOK_CLIENT_KEY=your_tiktok_client_key
# TIKTOK_CLIENT_SECRET=your_tiktok_client_secret
